  "dependencies": {
    "apollo-server-express": "^2.19.2",
    "body-parser": "^1.19.0",
    "dataloader": "^2.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "graphql-depth-limit": "^1.1.0",
    "graphql-playground-middleware-express": "^1.7.22",
    "graphql-query-complexity": "^0.7.2",
    "http": "0.0.1-security",
