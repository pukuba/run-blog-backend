{
  "name": "graphql-ts-boilerplate",
  "version": "1.0.0",
  "description": "graphql-ts-boilerplate",
  "main": "src/index.ts",
  "scripts": {
    "test": "NODE_PATH=src/ ts-mocha -p ./tsconfig.json src/TEST/*.test.ts --exit",
